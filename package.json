{
  "name": "squarebox",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-unused-imports": "^1.1.1",
    "lerna": "^4.0.0",
    "prettier": "^2.3.2",
    "qr-scanner": "^1.2.0",
    "react-qr-reader": "^2.2.1",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "setup": "yarn lerna bootstrap",
    "start:frontend": "npx lerna exec --scope=squarebox-frontend yarn start",
    "start:backend": "npx lerna exec --scope=squarebox-backend yarn start",
    "compile": "npx lerna exec -- yarn compile",
    "compile:fix": "yarn compile && yarn lint:fix",
    "lint": "npx lerna exec -- yarn lint",
    "lint:fix": "npx lerna exec -- yarn lint:fix"
  }
}
